<h1>New ingredient</h1>

<div id="available-foods">
  <p>Choose one food</p>
  <span class="available-foods-list">
    <% @foods.each do |food| %>
      <p>
        <strong>Name:</strong>
        <%= food.name %>
      </p>
    <% end %>
  </span>
</div>

<p>Add new food</p>

<div>
  <%= link_to "Back to recipes", recipes_path %>
</div>
